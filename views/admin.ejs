<!DOCTYPE html>
<html>
<head>
<title> Admin Dashboard </title>
</head>
<body>
Added Data â†“ <br>
<div id="mainDiv">
<% data.forEach(dtaa => { %>
<%= dtaa.name%> = <%= dtaa.value%>
<% }); %>
</div>
<br>
<input id="name"></input> <br>
<input id="val"></input> <br>
<button id="addBtn">Add Data</button>
<script>
document.getElementById("addBtn").addEventListener("click", () => {
var name = document.getElementById("name");
var value = document.getElementById("val");
if(!name.value || !value.value){ return; }

var mainDiv = document.getElementById("mainDiv");
var newDiv = document.createElement("div");
var htmlTemplate = `${name.value} = ${val.value}`;
newDiv.innerHTML = htmlTemplate;
mainDiv.appendChild(newDiv);

name.value = "";
value.value = "";
});
</script>
</body>
</html>
